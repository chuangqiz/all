.el-header {
  background-color: #292f4c;
  color: #fff;
  line-height: 60px;
  font-size: 24px;
}
.el-aside {
  background-color: #292f4c;
  color: #fff;
  height: calc(100vh - 60px);
}
.el-main {
  background-color: #E9EEF3;
  color: #333;
  text-align: center;
  height: calc(100vh - 60px);
}
.el-menu .el-menu-item {
  color: #fff;
  width: 100%;
  background-color: #292f4c;
}
